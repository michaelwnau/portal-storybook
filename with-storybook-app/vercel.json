{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build",
  "devCommand": "npm run storybook",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "storybook-static",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/storybook-static/$1"
    }
  ]
}
